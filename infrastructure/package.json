{
  "name": "@ec2-manager/infrastructure",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "synth": "cdk synth",
    "deploy": "cdk deploy",
    "deploy:dev": "cdk deploy --profile jnicamzn-sso-ec2",
    "deploy:prod": "cdk deploy --profile jnicamzn-sso-ec2 --context environment=production",
    "diff": "cdk diff",
    "destroy": "cdk destroy",
    "test": "jest",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"lib/**/*.ts\" \"bin/**/*.ts\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.214.0",
    "constructs": "^10.3.0"
  },
  "devDependencies": {
    "@aws-sdk/client-api-gateway": "^3.470.0",
    "@aws-sdk/client-cloudformation": "^3.470.0",
    "@aws-sdk/client-cloudwatch": "^3.470.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.470.0",
    "@aws-sdk/client-dynamodb": "^3.470.0",
    "@aws-sdk/client-s3": "^3.470.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "aws-cdk": "^2.214.0",
    "aws-cdk-lib": "^2.214.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0"
  }
}