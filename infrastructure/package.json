{
  "name": "@ec2-manager/infrastructure",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "synth": "cdk synth --profile jnicamzn-sso-ec2",
    "deploy": "echo 'ERROR: Use npm run deploy:dev or npm run deploy:prod instead' && exit 1",
    "deploy:dev": "../scripts/deploy.sh deploy dev",
    "deploy:prod": "../scripts/deploy.sh deploy prod",
    "diff": "cdk diff --profile jnicamzn-sso-ec2",
    "destroy": "echo 'ERROR: Use npm run destroy:dev or npm run destroy:prod instead' && exit 1",
    "destroy:dev": "../scripts/deploy.sh destroy dev",
    "destroy:prod": "../scripts/deploy.sh destroy prod",
    "validate": "../scripts/deploy.sh validate",
    "test": "jest",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"lib/**/*.ts\" \"bin/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "seed-users": "npx ts-node ../scripts/seed-users.ts",
    "seed-users:dev": "NODE_ENV=dev npx ts-node ../scripts/seed-users.ts",
    "seed-users:prod": "NODE_ENV=prod npx ts-node ../scripts/seed-users.ts"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.214.0",
    "constructs": "^10.3.0"
  },
  "devDependencies": {
    "@aws-sdk/client-api-gateway": "^3.470.0",
    "@aws-sdk/client-cloudformation": "^3.470.0",
    "@aws-sdk/client-cloudwatch": "^3.470.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.470.0",
    "@aws-sdk/client-dynamodb": "^3.470.0",
    "@aws-sdk/client-s3": "^3.470.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "aws-cdk": "^2.214.0",
    "aws-cdk-lib": "^2.214.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  }
}
